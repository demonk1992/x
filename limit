sudo apt-get install cron -y
cd /usr/bin
wget https://raw.githubusercontent.com/demonk1992/x/main/autokill.sh
wget https://raw.githubusercontent.com/demonk1992/x/main/userlimt.sh
wget https://raw.githubusercontent.com/demonk1992/x/main/userlimitssh.sh
wget https://raw.githubusercontent.com/demonk1992/x/main/clearcache.sh
chmod +x userlimit.sh
chmod +x userlimitssh.sh
chmod +x autokill.sh
chmod +x clearcache.sh
cd /etc
wget https://raw.githubusercontent.com/demonk1992/x/main/crontab
chmod +x crontab
cd
echo "@reboot root /usr/bin/userlimit.sh" > /etc/cron.d/userlimit
echo "@reboot root /usr/bin/userlimitssh.sh" > /etc/cron.d/userlimitssh
echo "@reboot root /usr/bin/clearcache.sh" > /etc/cron.d/clearcache
sed -i '$ i\screen -AmdS check /usr/bin/autokill.sh' /etc/rc.local